{
  "name": "synetta",
  "version": "0.1.0",
  "type": "module",
  "author": "Mikkel RINGAUD <mikkel@milescode.dev> (https://github.com/Vexcited)",
  "repository": "github:Vexcited/Synetta",
  "scripts": {
    "build": "rm -rf dist && tsc && tsx build.ts"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffect": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "default": "./dist/jsx-runtime.js"
    },
    "./bindings": {
      "types": "./dist/bindings/index.d.ts",
      "default": "./dist/bindings/index.js"
    },
    "./bridge": {
      "types": "./dist/bridge/index.d.ts",
      "default": "./dist/bridge/index.js"
    }
  },
  "devDependencies": {
    "esbuild": "^0.20.2",
    "tsx": "^4.7.1",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "core-js": "^3.36.1",
    "solid-js": "^1.8.16"
  }
}