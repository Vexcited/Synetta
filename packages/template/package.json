{
  "name": "synetta-template",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsx build.ts",
    "sync": "mv ./dist/index.js ./src-java/src/main/resources/index.bundle.mjs",
    "start": "mvn -f ./src-java/pom.xml clean javafx:run",
    "all": "pnpm build && pnpm sync && pnpm start"
  },
  "devDependencies": {
    "esbuild": "^0.20.2",
    "esbuild-plugin-synetta": "workspace:*",
    "tsx": "^4.7.1",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "synetta": "workspace:^"
  }
}
